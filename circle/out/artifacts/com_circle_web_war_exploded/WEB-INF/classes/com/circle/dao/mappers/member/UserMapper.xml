<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.circle.dao.mappers.member.UserMapper">

    <select id="getUserByPassword" resultMap="com.circle.entity.member.User">
        select * from member_user where username = #{username} and password = #{password}
    </select>

</mapper>